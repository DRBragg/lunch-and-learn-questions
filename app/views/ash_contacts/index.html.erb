<% provide(:title, "Listing ash_contacts") %>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th>Trial</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @ash_contacts.each do |ash_contact| %>
      <tr>
        <td><%= ash_contact.email %></td>
        <td><%= ash_contact.trial %></td>
        <td>
          <div class="btn-group">
            <%= link_to ash_contact, class: "btn btn-xs btn-default" do %>
              <%= fa_icon("eye", text: "Show") %>
            <% end %>
            <%= link_to edit_ash_contact_path(ash_contact), class: "btn btn-xs btn-default" do %>
              <%= fa_icon("pencil", text: "Edit") %>
            <% end %>
            <%= link_to ash_contact, class: "btn btn-xs btn-danger", method: :delete, data: { confirm: "Are you sure you want to delete this ash_contact?" } do %>
              <%= fa_icon("trash-o", text: "Destroy") %>
            <% end %>
          </div>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to new_ash_contact_path, class: "btn btn-default" do %>
  <%= fa_icon("plus", text: "New Ash contact") %>
<% end %>
